> hpfilter      = function(x, mu = 100) {
+   y = x
+   n <- length(y)          # number of observations
+   I <- diag(n)            # creates an identity matrix
+   D <- diff(I,lag=1,d=2)  # second order differences
+   d <- solve(I + mu * crossprod(D) , y) # solves focs
+   d
+ }
